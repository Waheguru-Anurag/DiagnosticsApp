{
    "name": "Diagnostics App Extension",
    "version": "1.0",
    "permissions": ["tabs", "system.cpu", "alarms"],
    "externally_connectable": {
        "matches": ["http://0.0.0.0:8000/"]
    },   
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "manifest_version": 3
}