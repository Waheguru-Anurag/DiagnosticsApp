{
    "name": "Diagnostics App Extension",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "declarativeContent", "system.cpu", "alarms"],
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["content_script.js"]
        }
    ],     
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
    "manifest_version": 2
}